#include<stdio.h>
int main()
{
int regnumber[]={1234,5678,9012,9872};
int targetregnumber=9012;
int size=sizeof(regnumber)/sizeof(regnumber[0]);
int found=0,i;
for(i=0;i<size;i++)
{
if(regnumber[i]==targetregnumber)
{
found=1;
break;
}
}
if(found)
{
printf("registrtation number %d found \n",targetregnumber);
}
else
{
printf("registrtation number %d not found \n",targetregnumber);
}
}
